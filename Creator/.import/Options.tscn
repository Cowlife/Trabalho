var song2
var currentSong = song1

func _ready():
   set_process(true)
   song2 = load(\"res://Little Things - Niwel [Vlog No Copyright Music].ogg\")

func _process(delta):
   if(self.is_playing() != true):
      if(self.get_stream() == song1):
         self.set_stream(song2)
      else:
         self.set_stream(song1)
      self.play()   
